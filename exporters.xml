<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE exporters SYSTEM "dtds/export.dtd">
<exporters>
        <exporter>
                <key>XmlOntologyExporter</key>
                <classpath>com.modica.ontobuilder.exports.XmlOntologyExporter</classpath>
                <exClass>Ontology</exClass>
                <type>XML</type>
                <extension>xml</extension>
		<shortDescription>XML</shortDescription>
		<longDescription>Export ontology to XML file</longDescription>
                <icon>xml.gif</icon>
        </exporter>
        <exporter>
        	<key>BizTalkOntologyExporter</key>
                <classpath>com.modica.ontobuilder.exports.BizTalkOntologyExporter</classpath>
                <exClass>Ontology</exClass>
                <type>BizTalk</type>
                <extension>xml</extension>
		<shortDescription>BizTalk</shortDescription>
		<longDescription>Export ontology to MS BizTalk file</longDescription>
                <icon>biztalk.gif</icon>
        </exporter>
        <exporter>
        	<key>BinaryOntologyExporter</key>
                <classpath>com.modica.ontobuilder.exports.BinaryOntologyExporter</classpath>
                <exClass>Ontology</exClass>
                <type>TXT</type>
                <extension>txt</extension>
		<shortDescription>TXT</shortDescription>
		<longDescription>Export ontology to binary file</longDescription>
                <icon>text.gif</icon>
        </exporter>
        <exporter>
        	<key>BestMatchingExporter</key>
                <classpath>com.modica.ontobuilder.exports.BestMatchingExporter</classpath>
                <exClass>Matching</exClass>
                <type>Default XML</type>
                <extension>xml</extension>
		<shortDescription>XML matching exporter</shortDescription>
		<longDescription>Export best matching to xml file</longDescription>
                <icon>xml.gif</icon>
        </exporter>
</exporters>