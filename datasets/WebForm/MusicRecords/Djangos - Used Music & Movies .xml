<source url="http://www.djangomusic.com/" interurl="http://www.djangomusic.com/search_advanced.asp?djc=DJ2412817424423251502211521015DT28DTWW42442C2185ZW5Z0IGIBWWB5IGAD">
  <srcname> Djangos - Used Music &amp; Movies </srcname>
  <form>
    <attrgroup>
      <attr name="Keywords or Phrase:" operator="yes" ename="keyword">
        <operator format="selection_list">
          <value>Begins With</value>
          <value>Contains Keyword(s)</value>
          <value>Contains Phrase</value>
          <value>Exact Match</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Item:" ename="item">
        <domain format="selection_list">
          <value>Artist/Group</value>
          <value>Album Title</value>
          <value>Song Track</value>
          <value>Movie Title</value>
          <value>Actor</value>
          <value>Director</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Media:" ename="media">
        <domain format="selection_list">
          <value>All Types</value>
          <value>CD</value>
          <value>VHS</value>
          <value>DVD</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Condition:" ename="condition">
        <domain format="selection_list">
          <value>New or Used</value>
          <value>Used</value>
          <value>New</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Inventory:" ename="inventory">
        <domain format="selection_list">
          <value>All Inventory</value>
          <value>In Stock Only</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>