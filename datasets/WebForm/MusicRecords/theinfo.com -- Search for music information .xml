<source url="http://www.theinfo.com/" interurl="http://www.theinfo.com/music/">
  <srcname> theinfo.com -- Search for music information </srcname>
  <form name="Search Music Databases &amp; Reviews(Rock)--">
    <attrgroup>
      <attr name="Artist" ename="artist">
        <domain format="text_box" />
      </attr>
      <attr name="Album" ename="album">
        <domain format="text_box" />
      </attr>
      <attr name="Song" ename="song">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="check_box">
          <option>All Music Guide</option>
          <option>CDDB ("world's largest online CD database")</option>
          <option>Launch (no song search)</option>
          <option>Q (reviews 1986 to present; no search by song)</option>
          <option>NME ("UK's biggest online archive of music news", reviews 1990 to present)</option>
          <option>Rolling Stone</option>
          <option>Ultimate Band List (no song search)</option>
          <option>Yahoo! Music (uses Muze music database)</option>
        </domain>
      </attr>
    </attrgroup>
  </form>
  <form name="Online Music Store Search">
    <attrgroup>
      <attr name="Artist" ename="artist">
        <domain format="text_box" />
      </attr>
      <attr name="Album" ename="album">
        <domain format="text_box" />
      </attr>
      <attr name="Song" ename="song">
        <domain format="text_box" />
      </attr>
      <attr name="Soundtrack" ename="soundtrack">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="check_box">
          <option>Amazon</option>
          <option>CD Now</option>
          <option>CD Universe</option>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>