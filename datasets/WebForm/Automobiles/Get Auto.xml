<source url="http://www2.getauto.com/" interurl="http://www2.getauto.com/search/dealer_car_search_back2.cfm?x_dealer_id=AADdr">
  <srcname>Get Auto</srcname>
  <form>
    <attrgroup>
      <attr name="Make" ename="make">
        <domain format="selection_list">
          <value>Any</value>
          <value>ACURA</value>
          <value>AUDI</value>
          <value>BMW</value>
          <value>BUICK</value>
          <value>CADILLAC</value>
          <value>CHEVROLET</value>
          <value>CHRYSLER</value>
          <value>DAEWOO</value>
          <value>DODGE</value>
          <value>FORD</value>
          <value>GMC</value>
          <value>HONDA</value>
          <value>HYUNDAI</value>
          <value>INFINITI</value>
          <value>ISUZU</value>
          <value>JEEP</value>
          <value>KIA</value>
          <value>LAND ROVER</value>
          <value>LEXUS</value>
          <value>LINCOLN</value>
          <value>MAZDA</value>
          <value>MERCEDES-BENZ</value>
          <value>MERCURY</value>
          <value>MITSUBISHI</value>
          <value>NISSAN</value>
          <value>OLDSMOBILE</value>
          <value>PLYMOUTH</value>
          <value>PONTIAC</value>
          <value>SAAB</value>
          <value>SATURN</value>
          <value>SUBARU</value>
          <value>TOYOTA</value>
          <value>VOLKSWAGEN</value>
          <value>VOLVO</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model" ename="model">
        <domain format="selection_list">
          <value>Choose a Model [optional]</value>
          <value>318</value>
          <value>328</value>
          <value>740</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Or, enter some keywords" ename="keywords">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Year is" level="1" ename="year">
        <input>
          <operator format="radio_button">
            <value>Any</value>
            <value>Newer Than</value>
            <value>Older Than</value>
            <value>Exactly</value>
          </operator>
          <domain format="selection_list">
            <value>Any</value>
            <value>2001</value>
            <value>2000</value>
            <value>1999</value>
            <value>1998</value>
            <value>1997</value>
            <value>1996</value>
            <value>1995</value>
            <value>1994</value>
            <value>1993</value>
            <value>1992</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Body Type" ename="body type">
        <domain format="selection_list">
          <value>Choose a Body Type</value>
          <value>2 Door Cars</value>
          <value>4 Door Cars</value>
          <value>Convertibles</value>
          <value>Minivan/Vans</value>
          <value>Pickup/Trucks</value>
          <value>Sports Utility Vehicles</value>
          <value>Wagons</value>
          <value>Other</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price Range" ename="price range">
        <domain format="selection_list">
          <value>Any</value>
          <value>1 - 5,000</value>
          <value>5,000 - 10,000</value>
          <value>10,000 - 15,000</value>
          <value>15,000 - 20,000</value>
          <value>20,000 - 25,000</value>
          <value>25,000 +</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>