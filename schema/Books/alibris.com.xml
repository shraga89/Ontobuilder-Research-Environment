<source url="http://www.alibris.com" interurl="http://www.alibris.com/search/search.cfm?S=R">
  <srcname>alibris.com</srcname>
  <form>
    <attrgroup>
      <attr name="Author" ename="author">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Title" ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Subject" ename="subject">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Any Word(s)" ename="any word">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN" ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name=" Alibris I.D.: " ename="alibris I.D.">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Binding: " ename="binding">
        <domain format="selection_list">
          <value>All Bindings</value>
          <value>Hardcover</value>
          <value>Softcover</value>
          <value>Audiobook</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Added Within " ename="added within">
        <domain format="selection_list">
          <value>All Time</value>
          <value>1 days</value>
          <value>7 days</value>
          <value>14 days</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name=" Language" ename="language">
        <domain format="text_box">
          <value>Any Language</value>
          <value>Arabic</value>
          <value>Basque</value>
          <value>Chinese</value>
          <value>Czech</value>
          <value>Danish</value>
          <value>Dutch</value>
          <value>Egyptian</value>
          <value>English</value>
          <value>Finnish</value>
          <value>French</value>
          <value>German</value>
          <value>Hebrew</value>
          <value>Hungarian</value>
          <value>Icelandic</value>
          <value>Italian</value>
          <value>Japanese</value>
          <value>Korean</value>
          <value>Latin</value>
          <value>Norwegian</value>
          <value>Polish</value>
          <value>Portuguese</value>
          <value>Russian</value>
          <value>Sanskrit</value>
          <value>Spanish</value>
          <value>Swedish</value>
          <value>Thai</value>
          <value>Turkish</value>
          <value>Vietnamese</value>
          <value>Yiddish</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price:" level="1" ename="price">
        <input>
          <operator format="text" name="from US$" ename="from" />
          <domain format="text_box" />
        </input>
        <input>
          <operator format="text" name="to US$" ename="to" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Publication Year:" level="1" ename="price">
        <input>
          <operator format="text" name="after" ename="after" />
          <domain format="text_box" />
        </input>
        <input>
          <operator format="text" name="before" ename="before" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="check_box">
          <value> first edition </value>
          <value> signed </value>
          <value> with dustjacket</value>
          <value> Limit search to nonfiction </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Sort results by:" ename="sort by" removed="yes">
        <domain format="selection_list">
          <value>Condition</value>
          <value>Title</value>
          <value>Author</value>
          <value>Pub Date</value>
          <value>Price</value>
          <value>Title Reverse</value>
          <value>Author Reverse</value>
          <value>Pub Date Reverse</value>
          <value>Price Reverse</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>