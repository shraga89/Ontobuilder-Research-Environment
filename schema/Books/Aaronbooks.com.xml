<source url="http://www.aaronbooks.com" interurl="http://www.aaronbooks.com/power.asp">
  <srcname>Aaronbooks.com</srcname>
  <form>
    <attrgroup>
      <attr name="Keyword(s): " operator="yes" ename="keyword">
        <operator format="radio_button">
          <option> Any </option>
          <option> All </option>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Title:" ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Author" ename="author">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Category" ename="category">
        <domain format="selection_list">
          <value> All Category</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN" ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Display" ename="display" removed="yes">
        <domain format="selection_list">
          <value>10</value>
          <value>25</value>
          <value>50</value>
          <value>100</value>
        </domain>
      </attr>
      <additional> results at a time </additional>
    </attrgroup>
  </form>
</source>