<source url="http://www3.bookstore.washington.edu" interurl="http://www3.bookstore.washington.edu/Textc/TextWelcome.taf?txtQuarter=Spring&amp;txtSchool=UW&amp;search=new">
  <srcname>University Bookstore of UW</srcname>
  <form>
    <attrgroup>
      <attr name="UW Course Number" ename="uw course number">
        <domain format="text_box" />
        <additional>Example: ARCH350    </additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name=" UW SLN Code" ename="uw sln code">
        <domain format="text_box" />
        <additional> Example: 1121</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Author Last Name" ename="author last name">
        <domain format="text_box" />
        <additional> Example: TRACHTENBERG</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Title Key Words" ename="title">
        <domain format="text_box" />
        <additional> Example: ARCHITECTURE</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN" ename="isbn">
        <domain format="text_box" />
        <additional> Example: 0130447021</additional>
      </attr>
    </attrgroup>
  </form>
</source>