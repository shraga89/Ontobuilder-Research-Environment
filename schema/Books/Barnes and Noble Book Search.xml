<source url="http://search.barnesandnoble.com" interurl="http://search.barnesandnoble.com/booksearch/search.asp?userid=229MHWSIXP&amp;pcount=0">
  <srcname>Barnes and Noble Book Search</srcname>
  <form>
    <attrgroup>
      <attr name="Title of Book" ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Author's Name" ename="author">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Subject" ename="subject">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN" ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Format" ename="format">
        <domain format="selection_list">
          <value>All formats</value>
          <value>Hardcover</value>
          <value>Paperback</value>
          <value>e Book</value>
          <value>&gt;Books on cassette or CD</value>
          <value> Large print </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Age" ename="age">
        <domain format="selection_list">
          <value>All age ranges</value>
          <value>Baby thru preschool</value>
          <value>4 thru 8 years</value>
          <value>9 thru 12 years</value>
          <value> Teens</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Subject" ename="subject">
        <domain format="selection_list">
          <value>all subjects</value>
          <value>------------</value>
          <value>Art, Architecture and Photography</value>
          <value>Business</value>
          <value>Children</value>
          <value>Computers</value>
          <value>Cooking</value>
          <value>Diet &amp; Health</value>
          <value>Entertainment</value>
          <value>Fiction and Literature</value>
          <value>History</value>
          <value>Home and Garden</value>
          <value>Medical</value>
          <value>Nonfiction</value>
          <value>Poetry</value>
          <value>Reference</value>
          <value>Religion</value>
          <value>Science and Nature</value>
          <value>Self-Improvement &amp; Inspiration</value>
          <value>Sports and Adventure</value>
          <value>Travel</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price" ename="price">
        <domain format="selection_list">
          <value>all prices</value>
          <value>----------------</value>
          <value>under $10.00 </value>
          <value>under $25.00 </value>
          <value>under $50.00 </value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>