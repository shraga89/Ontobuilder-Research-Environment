<source url="http://www.circlebooks.com.au/" interurl="http://www.circlebooks.com.au/">
  <srcname>circleBooks Online</srcname>
  <form>
    <attrgroup>
      <attr name="Search for a Book Title: Omit A,An,The " ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Author's Surname: " ename="author's surname">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Keyword:" ename="keyword">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN" ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Section" ename="section">
        <domain format="selection_list">
          <value>--select one--</value>
          <value>Ancient Past</value>
          <value>Body Therapies</value>
          <value>Contemporary Teachers</value>
          <value>Earth &amp; Life Energies</value>
          <value>Esoteric &amp; Occult Arts</value>
          <value>Esoteric &amp; Occult Tradition</value>
          <value>Health &amp; Healing</value>
          <value>Mind &amp; Spirit Therapies</value>
          <value>Received Teachings</value>
          <value>Sacred &amp; Mystical Tradition</value>
          <value>Unexplained &amp; Mysteries</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Display search results" ename="display search result" removed="yes">
        <domain format="selection_list">
          <value>5</value>
          <value>10</value>
          <value>20</value>
          <value>50</value>
        </domain>
        <additional>books at a time </additional>
      </attr>
    </attrgroup>
  </form>
</source>