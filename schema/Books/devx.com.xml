<source url="http://archive.devx.com" interurl="http://archive.devx.com/free/books/booksearch.asp">
  <srcname>devx.com</srcname>
  <form>
    <attrgroup>
      <attr name="Title " ename="title">
        <domain format="text_box" />
      </attr>
      <attr name="Author Last Name " ename="author last name">
        <domain format="text_box" />
      </attr>
      <attr name="ISBN " ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Category " ename="category">
        <domain format="check_box">
          <value>VB </value>
          <value>Java</value>
          <value> C++ </value>
          <value>Enterprise </value>
          <value>Web </value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>