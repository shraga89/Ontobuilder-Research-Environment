<source url="http://www.briansbooks.com/" interurl="http://www.briansbooks.com/catalog/books/srch/query?sIeTN9rD">
  <srcname>brian's books</srcname>
  <form>
    <attrgroup>
      <attr name="ISBN" operator="yes" ename="isbn">
        <operator format="radio_button">
          <option> All </option>
          <option> Any Terms  </option>
        </operator>
        <operator format="check_box">
          <option> match substring </option>
        </operator>
        <domain format="text_box" />
      </attr>
      <attr name=" Author" operator="yes" ename="author">
        <operator format="radio_button">
          <option> All </option>
          <option> Any Terms  </option>
        </operator>
        <operator format="check_box">
          <option> match substring </option>
        </operator>
        <domain format="text_box" />
      </attr>
      <attr name=" Title Key Word " operator="yes" ename="title">
        <operator format="radio_button">
          <option> All </option>
          <option> Any Terms  </option>
        </operator>
        <operator format="check_box">
          <option> match substring </option>
        </operator>
        <domain format="text_box" />
      </attr>
      <attr name="Key Word " operator="yes" ename="key word">
        <operator format="radio_button">
          <option> All </option>
          <option> Any Terms  </option>
        </operator>
        <operator format="check_box">
          <option> match substring </option>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Matches per page" ename="matches per page" removed="yes">
        <domain format="selection_list">
          <value>1</value>
          <value>5</value>
          <value>10</value>
          <value>25</value>
          <value>50</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>