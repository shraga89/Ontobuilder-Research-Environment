<source url="http://www.fabmall.com/fabmarthome.asp" interurl="http://www.fabmall.com/Stores/misc/frmMainFrame.asp?Files=ADV&amp;catalogId=Books">
  <srcname>fabmall.com Book Search</srcname>
  <form>
    <attrgroup>
      <attr name="Search in :" ename="">
        <domain format="radio_button">
          <value>Books </value>
          <value>E-Books</value>
          <value>Magazines </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Book name  " operator="yes" ename="book name">
        <operator format="radio_button">
          <option>Starts with </option>
          <option>Contains</option>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Authors name or part of authors name " ename="author name">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Category " ename="category">
        <domain format="selection_list">
          <value> Error: frameset </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Format " ename="format">
        <domain format="selection_list">
          <value> Error: frameset </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Language" ename="language">
        <domain format="selection_list">
          <value> Error: frameset </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price Range " ename="price">
        <domain format="selection_list">
          <value> Error: frameset </value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN " ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>