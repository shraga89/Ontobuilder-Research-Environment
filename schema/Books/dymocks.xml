<source url="http://www.dymocks.com.au" interurl="http://www.dymocks.com.au/search/SearchAdvanced.asp">
  <srcname>dymocks</srcname>
  <form>
    <attrgroup>
      <attr name="Title " operator="yes" ename="title">
        <operator format="radio_button">
          <option> All words </option>
          <option> Any words </option>
          <option> Exact Title  </option>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Author " ename="author">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Department" ename="department">
        <domain format="selection_list">
          <value>All</value>
          <value>All</value>
          <value>Art and Leisure</value>
          <value>Business and Management</value>
          <value>Children</value>
          <value>Computers</value>
          <value>Education and Reference</value>
          <value>Fiction</value>
          <value>Health Sport and Motivation</value>
          <value>Home and Garden</value>
          <value>Non Fiction</value>
          <value>Travel</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="ISBN" ename="isbn">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>