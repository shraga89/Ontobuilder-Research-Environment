<source url="http://www.abc.nl/" interurl="http://www.abc.nl/">
  <srcname>American Book Center</srcname>
  <form>
    <attrgroup>
      <attr name="keyword(s) in title" ename="title">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="author(s)" ename="author">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="published" ename="publication date">
        <input>
          <operator name="" ename="after" />
          <domain format="text_box" />
        </input>
        <input>
          <operator name="before" ename="before" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="subject">
        <input>
          <domain format="selection_list">
            <value>All subjects</value>
            <value>Antiques and Collectibles</value>
            <value>Architecture</value>
            <value>Art</value>
            <value>Biography and Autobiography</value>
            <value>Body, Mind and Spirit</value>
            <value>Business and Economics</value>
            <value>Computers</value>
            <value>Cooking</value>
            <value>Crafts</value>
            <value>Education</value>
            <value>Family and Health</value>
            <value>Fiction</value>
            <value>Foreign Language Study</value>
            <value>Games and Hobbies</value>
            <value>Gardening</value>
            <value>History</value>
            <value>House and Home</value>
            <value>Humour</value>
            <value>Juvenile Fiction</value>
            <value>Juvenile Nonfiction</value>
            <value>Language Arts and Disciplines</value>
            <value>Law</value>
            <value>Literary Collections</value>
            <value>Literary Criticims</value>
            <value>Mathematics</value>
            <value>Medical</value>
            <value>Miscellanious</value>
            <value>Music</value>
            <value>Nature and Biology</value>
            <value>Performing Arts</value>
            <value>Philosophy</value>
            <value>Photography</value>
            <value>Political Science</value>
            <value>Psychology</value>
            <value>Reference</value>
            <value>Religion</value>
            <value>Science</value>
            <value>Self Help</value>
            <value>Social Science</value>
            <value>Sports and Recreation</value>
            <value>Technology</value>
            <value>Transportation</value>
            <value>Travel</value>
            <value>True Stories</value>
            <value>Unknown</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="publisher">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
  </form>
</source>