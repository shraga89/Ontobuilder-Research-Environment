<source url="http://www.britannia-music.co.uk/" interurl="http://www.britannia-music.co.uk/AdvancedAudioSearchInput.do">
  <srcname> Britannia Music and Video - Music </srcname>
  <form name="music">
    <attrgroup>
      <attr name="Catalogue number" ename="catalog #">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Artist name" ename="artist">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Album title" ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Musical genre" ename="genre">
        <domain format="selection_list">
          <value>All</value>
          <value>Ballet</value>
          <value>Blues</value>
          <value>Children</value>
          <value>Choral Works</value>
          <value>Christmas</value>
          <value>Classical</value>
          <value>Collections</value>
          <value>Contemporary</value>
          <value>Country</value>
          <value>Country Compilation</value>
          <value>Dance</value>
          <value>Dance Compilation</value>
          <value>Early Music</value>
          <value>Easy Listening</value>
          <value>Easy Compilation</value>
          <value>Indie</value>
          <value>Jazz</value>
          <value>Jazz \&amp; Nostalgia</value>
          <value>Musical Theatre</value>
          <value>Opera</value>
          <value>Pop</value>
          <value>Pop Compilation</value>
          <value>Reggae</value>
          <value>Relaxation</value>
          <value>Rock</value>
          <value>Rock Compilation</value>
          <value>Soul</value>
          <value>Soul Compilation</value>
          <value>Film Soundtrack</value>
          <value>Self-Help</value>
          <value>Soundtrack</value>
          <value>Spoken Word</value>
          <value>Traditional/Fold/World</value>
          <value>World Music</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Composer" ename="composer">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Conductor" ename="conductor">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Format" ename="format">
        <domain format="check_box">
          <option>CD</option>
          <option>Cassette</option>
          <option>Music DVD</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Recording quality" ename="recording quality">
        <domain format="selection_list">
          <value>All</value>
          <value>AAD</value>
          <value>ADD</value>
          <value>DDD</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>