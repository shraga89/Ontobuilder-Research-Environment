<source url="http://secure01.entertainme.com/" interurl="http://secure01.entertainme.com/servlet/LandingPageServlet?landingPageName=search">
  <srcname> EntertainMe.com </srcname>
  <form name="MUSIC SEARCH:">
    <attrgroup>
      <attr name="Music Titles" operator="yes" ename="title">
        <operator format="selection_list">
          <value>Close Match</value>
          <value>Exact Match</value>
        </operator>
        <domain format="text_box" />
      </attr>
      <attr name="Music Artists" operator="yes" ename="artist">
        <operator format="selection_list">
          <value>Close Match</value>
          <value>Exact Match</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="radio_button">
          <value>All Formats (LP,DVD Audio...)</value>
          <value>CD</value>
          <value>Cassette</value>
          <value>DVD Audio</value>
          <value>SACD</value>
          <value>Vinyl (LP)</value>
          <value>VHS Audio</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>