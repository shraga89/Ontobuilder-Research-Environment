<source url="http://www.expresspersonnel.com/" interurl="http://www.expresspersonnel.com/jobsearchadv.asp">
  <srcname>Express Personnel - Job Search. Employment. Franchises</srcname>
  <form>
    <attrgroup>
      <attr name="Choose Country" ename="country">
        <domain format="radio_button">
          <value>USA</value>
          <value>Canada</value>
          <value>South Africa</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Select Job Categories" ename="job categories">
        <domain format="check_box">
          <option>Search All Categories</option>
          <option>Accounting Financial</option>
          <option>Engineering/Manufacturing</option>
          <option>Health Care</option>
          <option>Home Help</option>
          <option>Human Resources</option>
          <option>Industrial</option>
          <option>Information Technology</option>
          <option>Office Services</option>
          <option>Professional</option>
          <option>Sales</option>
          <option>Marketing</option>
          <option>Technical</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Enter Keywords to Search" ename="keywords">
        <additional>(separate by commas)</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>