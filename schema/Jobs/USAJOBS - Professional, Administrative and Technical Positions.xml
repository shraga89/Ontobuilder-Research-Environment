            <value>Arts, Entertainment &amp; Media</value>
            <value>Contracting and Print Management</value>
            <value>Education and Training</value>
            <value>Human Resources</value>
            <value>Marketing, Communications, Advertising &amp; Public Relations</value>
            <value>Mathematics and Statistics</value>
            <value>Program Analysis</value>
            <value>Program Mgmt / Public Administration</value>
            <value>Property and Supply Management</value>
            <value>Public Buildings and Historic Sites</value>
            <value>Real Estate and Planning</value>
          </domain>
        </input>
        <input>
          <operator name="NATURAL RESOURCES" ename="natural resources" />
          <domain format="radio_button">
            <value>Conservation</value>
            <value>Environmental Management</value>
            <value>Fish &amp; Wildlife Management</value>
            <value>Forests and Parks</value>
            <value>Land and Range Management</value>
            <value>Recreation &amp; Culture</value>
            <value>Resource Management and Surveys</value>
            <value>Resource Safety &amp; Preservation</value>
          </domain>
        </input>
        <input>
          <operator name="" ename="" />
          <domain format="radio_button">
            <value>ENGINEERING &amp; ARCHITECTURE</value>
          </domain>
        </input>
        <input>
          <operator name="FINANCIAL" ename="financial" />
          <domain format="radio_button">
            <value>Accounting, Auditing &amp; Taxation</value>
            <value>Banking, Budgeting, Finance &amp; Economics</value>
            <value>Monetary Compliance</value>
          </domain>
        </input>
        <input>
          <operator name="SCIENCES" ename="sciences" />
          <domain format="radio_button">
            <value>Biology</value>
            <value>Earth and Ocean Studies</value>
            <value>Physical Science</value>
            <value>Research and Consumer Protection</value>
            <value>Social Science &amp; Welfare</value>
            <value>Space Exploration</value>
          </domain>
        </input>
        <input>
          <operator name="FOREIGN AFFAIRS" ename="foreign affairs" />
          <domain format="radio_button">
            <value>Diplomacy</value>
            <value>Trade and Commerce</value>
          </domain>
        </input>
        <input>
          <operator name="TRANSPORTATION" ename="transportation" />
          <domain format="radio_button">
            <value>Industry</value>
            <value>Security and Safety</value>
          </domain>
        </input>
        <input>
          <operator name="" ename="" />
          <domain format="radio_button">
            <value>INSURANCE AND BENEFITS</value>
          </domain>
        </input>
        <input>
          <operator name="TECHNICIAN POSITIONS" ename="technician position" />
          <domain format="radio_button">
            <value>Medical Technicians</value>
            <value>Scientific Technicians</value>
            <value>Other Technicians</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Enter a Job Title Keyword" ename="job title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Select Your Desired Salary" ename="desired salary">
        <domain format="radio_button">
          <value>$19,000 - $25,000</value>
          <value>$22,000 - $39,000</value>
          <value>$33,000 - $62,000</value>
          <value>$57,000 - $88,000</value>
