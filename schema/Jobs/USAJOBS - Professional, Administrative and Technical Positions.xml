<source url="http://www.usajobs.opm.gov/" interurl="http://www.usajobs.opm.gov/a1.htm">
  <srcname>USAJOBS - Professional, Administrative and Technical Positions</srcname>
  <form>
    <attrgroup>
      <attr name="Professional, Administrative and Technical Positions" level="1" ename="position">
        <input>
          <operator name="AGRICULTURE &amp; FOOD INDUSTRY" ename="agriculture and food industry" />
          <domain format="radio_button">
            <value>Agricultural Research and Marketing</value>
            <value>Farm Management</value>
            <value>Food Safety and Inspection</value>
          </domain>
        </input>
        <input>
          <operator name="LAW &amp; PROTECTION" ename="law and protection" />
          <domain format="radio_button">
            <value>Investigations &amp; Evidence Analysis</value>
            <value>Law Enforcement and Compliance</value>
            <value>Legal, Regulatory, &amp; Judicial</value>
            <value>Safety and Security</value>
          </domain>
        </input>
        <input>
          <operator name="" ename="" />
          <domain format="radio_button">
            <value>ARCHIVES, LIBRARIES &amp; MUSEUMS</value>
          </domain>
        </input>
        <input>
          <operator name="MEDICINE" ename="medicine" />
          <domain format="radio_button">
            <value>Medical Research</value>
            <value>Physicians, Dentists, Nurses &amp; Assistants</value>
            <value>Public Health</value>
            <value>Veterinary Medicine</value>
          </domain>
        </input>
        <input>
          <operator name="BUSINESS" ename="business" />
