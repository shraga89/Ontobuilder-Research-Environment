<source url="http://www.nonprofitjobs.org/" interurl="http://www.nonprofitjobs.org/sjob.cfm">
  <srcname>Community Career Center</srcname>
  <form>
    <attrgroup>
      <attr name="City:" ename="city">
        <additional>Select jobs by work location:</additional>
        <domain format="text_box" />
