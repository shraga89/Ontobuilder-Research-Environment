<source url="http://www.swiss.com/" interurl="http://www.swiss.com/">
  <srcname>Swiss Airlines</srcname>
  <form>
    <attrgroup>
      <attr name="From">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="To">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Dep. Date">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Ret. Date">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Passenger (Adults)" ename="adult">
        <input>
          <domain format="selection_list">
            <value>1 </value>
            <value>2 </value>
            <value>3 </value>
            <value>4 </value>
            <value>5 </value>
          </domain>
        </input>
      </attr>
    </attrgroup>
  </form>
</source>