<source url="http://www.cheapairlines.com" interurl="http://www.cheapairlines.com/flightsadv.html">
  <srcname>Cheap Airlines</srcname>
  <form>
    <attrgroup>
      <attr name="Departure City">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Arrival City">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Departure Date (mm/dd/yy)" ename="departure date">
        <input>
          <domain format="text_box" />
        </input>
        <input>
          <domain format="selection_list">
            <value>Morning</value>
            <value>Afternoon</value>
            <value>Evening</value>
            <value>1 am</value>
            <value>2 am</value>
            <value>3 am</value>
            <value>4 am</value>
            <value>5 am</value>
            <value>6 am</value>
            <value>7 am</value>
            <value>8 am</value>
            <value>9 am</value>
            <value>10 am</value>
            <value>11 am</value>
            <value>12 noon</value>
            <value>1 pm</value>
            <value>2 pm</value>
            <value>3 pm</value>
            <value>4 pm</value>
            <value>5 pm</value>
            <value>6 pm</value>
            <value>7 pm</value>
            <value>8 pm</value>
            <value>9 pm</value>
            <value>10 pm</value>
            <value>11 pm</value>
            <value>12 midnight</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Return Date (mm/dd/yy)" ename="return date">
        <input>
          <domain format="text_box" />
        </input>
        <input>
          <domain format="selection_list">
            <value>Morning</value>
            <value>Afternoon</value>
            <value>Evening</value>
            <value>1 am</value>
            <value>2 am</value>
            <value>3 am</value>
            <value>4 am</value>
            <value>5 am</value>
            <value>6 am</value>
            <value>7 am</value>
            <value>8 am</value>
            <value>9 am</value>
            <value>10 am</value>
            <value>11 am</value>
            <value>12 noon</value>
            <value>1 pm</value>
            <value>2 pm</value>
            <value>3 pm</value>
            <value>4 pm</value>
            <value>5 pm</value>
            <value>6 pm</value>
            <value>7 pm</value>
            <value>8 pm</value>
            <value>9 pm</value>
            <value>10 pm</value>
            <value>11 pm</value>
            <value>12 midnight</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Adults" ename="adult">
        <input>
          <domain format="selection_list">
            <value>1</value>
            <value>2</value>
            <value>3</value>
            <value>4</value>
          </domain>
        </input>
        <additional>(age 12 to 61)</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Children">
        <input>
          <domain format="selection_list">
            <value>0</value>
            <value>1</value>
            <value>2</value>
            <value>3</value>
            <value>4</value>
          </domain>
        </input>
        <additional> (age 2 to 11)</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Infants in Adult's Lap:" ename="infant">
        <input>
          <domain format="selection_list">
            <value>0</value>
            <value>1</value>
            <value>2</value>
            <value>3</value>
          </domain>
        </input>
        <additional> under age 2</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Seniors" ename="senior">
        <input>
          <domain format="selection_list">
            <value>0</value>
            <value>1</value>
            <value>2</value>
            <value>3</value>
          </domain>
        </input>
        <additional> (age 62 and over)</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Infants in Reserved Seats" ename="infant">
        <input>
          <domain format="selection_list">
            <value>0</value>
            <value>1</value>
            <value>2</value>
            <value>3</value>
          </domain>
        </input>
        <additional> (under age 2)</additional>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Class">
        <input>
          <domain format="radio_button">
            <value>Economy</value>
            <value>Business</value>
            <value>First</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="">
        <input>
          <domain format="check_box">
            <value>Avoid Change Penalties: </value>
            <value>No Advance Purchase Restrictions: </value>
            <value> Direct Flights Only:</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Preferred Airlines: (up to 3)" ename="airline">
        <input>
          <domain format="selection_list">
            <error> frameset, cannot get the values</error>
          </domain>
