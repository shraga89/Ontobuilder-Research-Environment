<source url="http://www.skyauction.com" interurl="http://www.skyauction.com/jsp/search.jsp">
  <srcname>SkyAuction.com</srcname>
  <form>
    <attrgroup>
      <attr name="Departing From" ename="depart from">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Destination">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Departure Date: (mm/dd/yyyy)" ename="departure date">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Category">
        <input>
          <domain format="selection_list">
            <value>All</value>
            <value>Europe</value>
            <value>Asia, Africa &amp; Australia</value>
            <value>USA &amp; Canada</value>
            <value>Latin America &amp; Mexico</value>
            <value>Sun Packages</value>
            <value>Buy It Now</value>
            <value>Cruises</value>
            <value>Last Minute Getaways</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Vacation Type">
        <input>
          <domain format="selection_list">
            <value>All</value>
            <value>Air</value>
            <value>Hotel Only</value>
            <value>Package</value>
            <value>Cruise</value>
            <value>Certificate</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
  </form>
</source>