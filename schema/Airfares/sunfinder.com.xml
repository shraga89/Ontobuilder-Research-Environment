<source url="http://www.sunfinder.com/farebeater.htm" interurl="http://www.sunfinder.com/farebeater.htm">
  <srcname>sunfinder.com</srcname>
  <form>
    <attrgroup>
      <attr name="Departure City">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Arrival City">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Departure Date">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Return Date">
        <input>
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Number of Tickets">
        <input>
          <domain format="selection_list">
            <value>1 ticket</value>
            <value>2 ticket</value>
            <value>3 ticket</value>
            <value>4 ticket</value>
            <value>5 ticket</value>
            <value>6 ticket</value>
            <value>7 ticket</value>
            <value>8 ticket</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
  </form>
</source>