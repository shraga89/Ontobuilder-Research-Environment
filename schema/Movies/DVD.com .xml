<source url="http://www.dvd.com/" interurl="http://www.dvd.com/advanced_search.asp">
  <srcname> DVD.com </srcname>
  <form name="Advanced Search">
    <attrgroup>
      <attr name="Category:" ename="category">
        <additional>Hold down the Ctrl key to select multiple categories</additional>
        <domain format="selection_list">
          <value>Action</value>
          <value>Adult</value>
          <value>Animation</value>
          <value>Anime</value>
          <value>Boxed sets</value>
          <value>Classic</value>
          <value>Classical music</value>
          <value>Comedy</value>
          <value>Concert</value>
          <value>Cult</value>
          <value>Documentary</value>
          <value>Drama</value>
          <value>DTS</value>
          <value>Family</value>
          <value>Foreign</value>
          <value>Hong Kong</value>
          <value>Horror</value>
          <value>IMAX</value>
          <value>Karaoke</value>
          <value>miscellaneous</value>
          <value>Mature</value>
          <value>Musicals</value>
          <value>New Age</value>
          <value>Health and Fitness</value>
          <value>Romance</value>
          <value>Sci-Fi</value>
          <value>Suspense</value>
          <value>Specialty</value>
          <value>Unreleased</value>
          <value>Western</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Keywords:" ename="keyword">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Title:" ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Actor:" ename="actor">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Director:" ename="director">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Producer:" ename="producer">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Writer:" ename="writer">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Studio:" ename="studio">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Rating:" ename="rating">
        <domain format="check_box">
          <option>G</option>
          <option>PG</option>
          <option>PG-13</option>
          <option>R</option>
          <option>NC-17</option>
          <option>NR</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="DVD Release Year:" operator="yes" ename="release date">
        <operator format="selection_list">
          <value>Before</value>
          <value>During</value>
          <value>After</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Theatrical Release Year:" operator="yes" ename="release date">
        <operator format="selection_list">
          <value>Before</value>
          <value>During</value>
          <value>After</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Other Languages:" ename="language">
        <domain format="selection_list">
          <value>Select Audio Track</value>
          <value>English</value>
          <value>Spanish</value>
          <value>French</value>
          <value>Italian</value>
          <value>Japanese</value>
          <value>German</value>
          <value>Dutch</value>
          <value>Swedish</value>
          <value>Russian</value>
          <value>Cantonese</value>
          <value>Mandarin</value>
          <value>Castilian</value>
          <value>Canadian French</value>
          <value>Catalan</value>
          <value>Korean</value>
          <value>Portuguese</value>
          <value>Vietnamese</value>
          <value>Greek</value>
          <value>Serbo-Croation</value>
          <value>Hebrew</value>
          <value>Arabic</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="selection_list">
          <value>Select Subtitle Language</value>
          <value>English</value>
          <value>Spanish</value>
          <value>French</value>
          <value>Italian</value>
          <value>Japanese</value>
          <value>German</value>
          <value>Dutch</value>
          <value>Swedish</value>
          <value>Cantonese</value>
          <value>Mandarin</value>
          <value>Castilian</value>
          <value>Catalan</value>
          <value>Korean</value>
          <value>Portuguese</value>
          <value>Vietnamese</value>
          <value>Greek</value>
          <value>Chinese (Simplified characters)</value>
          <value>Chinese (Traditional)</value>
          <value>Bahasa (Malaysian)</value>
          <value>Bahasa (Indonesian)</value>
          <value>Thai</value>
          <value>Canadian French</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Closed Captioning:" ename="caption">
        <domain format="selection_list">
          <value>no</value>
          <value>yes</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Regional Coding:" ename="region">
        <domain format="selection_list">
          <value>None</value>
          <value>Region 1</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Video Format:" ename="format">
        <additional>Hold down the Ctrl key to select multiple categories</additional>
        <domain format="selection_list">
          <value>Standard 1.33:1 [4:3]</value>
          <value>Widescreen</value>
          <value>Widescreen 1.66:1</value>
          <value>Widescreen 1.85:1</value>
          <value>Widescreen 2.35:1</value>
          <value>Widescreen 1.77:1</value>
          <value>Widescreen 2.20:1</value>
          <value>Filmed in IMAX</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Audio Encoding:" ename="audio encoding">
        <domain format="selection_list">
          <value>Mono</value>
          <value>Stereo</value>
          <value>Dolby Digital 5.1 Surround</value>
          <value>Dolby Digital 4.1 Surround</value>
          <value>Dolby Digital Surround</value>
          <value>Dolby Digital Stereo</value>
          <value>Dolby Digital Mono</value>
          <value>PCM 48khz 16-bit Stereo</value>
          <value>Chace Surround Stereo</value>
          <value>DTS 5.1 Surround Stereo</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="check_box">
          <option>Enhanced for 16x9</option>
          <option>THX Mastering</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="If you are choosing more than one special feature, which would you prefer?" operator="yes" ename="feature">
        <operator format="radio_button">
          <value>Search for movies that include ALL selected special features</value>
          <value>Search for movies that include ANY selected special features</value>
        </operator>
        <domain format="check_box">
          <option>Theatrical Trailer</option>
          <option>Additional Footage</option>
          <option>Audio Commentary</option>
          <option>Interactive Menu</option>
          <option>Production Notes</option>
          <option>Featurette</option>
          <option>Cast/Crew Bios</option>
          <option>Filmography</option>
          <option>Included Trivia Book</option>
          <option>Documentary</option>
          <option>Interviews</option>
          <option>Interactive Film Trivia</option>
          <option>Multiple Angles</option>
          <option>Web Access</option>
          <option>Interactive Games</option>
          <option>Film Highlights</option>
          <option>Music Video</option>
          <option>Story Board</option>
          <option>Discography</option>
          <option>Copy Protection</option>
          <option>Photo Gallery</option>
          <option>Parental Lock</option>
          <option>TV Spot</option>
          <option>Separate Film Score Audio</option>
          <option>Deleted Scenes</option>
          <option>Alternate Endings</option>
          <option>Behind-the-scenes Footage</option>
          <option>Original Screenplay</option>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>