<source url="http://www.movietickets.com/" interurl="http://www.movietickets.com/advanced_search.asp">
  <srcname> MovieTickets.com </srcname>
  <form>
    <attrgroup>
      <attr name="in the Movie Title" ename="title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Genre" ename="genre">
        <domain format="selection_list">
          <value>Any</value>
          <value>Action</value>
          <value>Adult</value>
          <value>Art/Foreign/Independent</value>
          <value>Classic</value>
          <value>Comedy</value>
          <value>Documentary</value>
          <value>Drama</value>
          <value>Family</value>
          <value>Horror</value>
          <value>Musical</value>
          <value>Romance</value>
          <value>Science Fiction</value>
          <value>Thriller/Suspense</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Ratings" ename="rating">
        <domain format="selection_list">
          <value>Any</value>
          <value>G</value>
          <value>PG</value>
          <value>PG (13)</value>
          <value>R</value>
          <value>NC (17)</value>
          <value>No</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Starring" ename="star">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Directed By" ename="director">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>