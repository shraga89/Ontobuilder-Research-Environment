<source url="http://www.musicvideodistributors.com/" interurl="http://www.musicvideodistributors.com/">
  <srcname> Music Video Distributors </srcname>
  <form>
    <attrgroup>
      <attr name="ARTIST / BAND / TITLE" operator="yes" ename="artist|band|title">
        <operator format="check_box">
          <option>- search descriptions also</option>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="FORMAT" ename="format">
        <domain format="selection_list">
          <value>All</value>
          <value>DVD</value>
          <value>VHS</value>
          <value>DVD-Audio</value>
          <value>T-Shirt</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="GENRE" ename="genre">
        <domain format="selection_list">
          <value>All</value>
          <value>---MUSIC---</value>
          <value>Adult Contemp/MOR</value>
          <value>Alternative/Punk</value>
          <value>Ballet</value>
          <value>Blues</value>
          <value>Classical</value>
          <value>Country</value>
          <value>Documentary</value>
          <value>Electronic</value>
          <value>Folk</value>
          <value>Black Gospel</value>
          <value>White Gospel</value>
          <value>Instructional</value>
          <value>Jazz</value>
          <value>Karaoke</value>
          <value>Kids Christian</value>
          <value>Latin</value>
          <value>Musical/Broadway</value>
          <value>New Age</value>
          <value>Opera</value>
          <value>Play</value>
          <value>Pop/Rock</value>
          <value>R &amp; B</value>
          <value>Reggae</value>
          <value>Metal</value>
          <value>Rap/Hip Hop</value>
          <value>Christian Rock</value>
          <value>Spanish language</value>
          <value>Spoken word</value>
          <value>World</value>
          <value>Holiday</value>
          <value>---MOVIES---</value>
          <value>Adult</value>
          <value>Action/Adventure</value>
          <value>Animation</value>
          <value>Comedy</value>
          <value>Drama</value>
          <value>Foreign Film</value>
          <value>Horror</value>
          <value>Health/Fitness</value>
          <value>Kids</value>
          <value>Martial Arts</value>
          <value>Anime/Manga</value>
          <value>Misc.</value>
          <value>Movie</value>
          <value>Mystery/Thriller</value>
          <value>Sci Fi</value>
          <value>Sports/Games</value>
          <value>Travel</value>
          <value>Television</value>
          <value>Urban Adult</value>
          <value>Western</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="check_box">
          <option>MVD EXCLUSIVES ONLY</option>
          <option>ONLY SEARCH UPCOMING RELEASES</option>
          <option>SEARCH BY RELEASE DATE</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="MVD SKU#" ename="sku #">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="UPC" ename="upc">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>