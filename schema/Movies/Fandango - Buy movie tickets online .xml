<source url="http://www.fandango.com/" interurl="http://www.fandango.com/advanced_search.asp">
  <srcname> Fandango - Buy movie tickets online </srcname>
  <form name="Search by theater partner">
    <attrgroup>
      <attr name="City or Zip Code:" ename="zipcode">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
