<source url="http://www.aint-it-cool-news.com/" interurl="http://www.aint-it-cool-news.com/search.html">
  <srcname> Ain't It Cool News </srcname>
  <form>
    <attrgroup>
      <attr name="Enter Keyword:" operator="yes" ename="keyword">
        <operator format="check_box">
          <option>Search Titles Only</option>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Scope of search:" ename="scope">
        <domain format="selection_list">
          <value>Full site search</value>
          <value>Search only coolnews</value>
          <value>Search only reviews</value>
          <value>Search only Coaxial</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Date limit: (mm/dd/yyyy)" level="2" ename="date limit">
        <attr name="From" level="1" ename="from">
          <attr name="" ename="">
            <domain format="text_box" />
          </attr>
          <attr name="" ename="">
            <domain format="text_box" />
          </attr>
          <attr name="" ename="">
            <domain format="text_box" />
          </attr>
        </attr>
        <attr name="To" level="1" ename="to">
          <attr name="" ename="">
            <domain format="text_box" />
          </attr>
          <attr name="" ename="">
            <domain format="text_box" />
          </attr>
          <attr name="" ename="">
            <domain format="text_box" />
          </attr>
        </attr>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Limit to" level="1" ename="limit to">
        <attr name="" ename="">
          <domain format="selection_list">
            <value>Search all genres</value>
            <value>Action / Adventure</value>
            <value>Comedies</value>
            <value>Dramas</value>
            <value>Horrors</value>
            <value>Sci-Fi</value>
            <value>Star Wars</value>
            <value>Suspense</value>
            <value>Animated</value>
            <value>Cult</value>
            <value>Bad Idea</value>
          </domain>
        </attr>
        <attr name="" ename="">
          <domain format="radio_button">
            <value>Rumors</value>
            <value>Confirmed News</value>
            <value>Don't care</value>
          </domain>
        </attr>
        <attr name="" ename="">
          <domain format="check_box">
            <option>No spoilers</option>
          </domain>
        </attr>
      </attr>
    </attrgroup>
  </form>
</source>