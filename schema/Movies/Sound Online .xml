<source url="http://www.absound.ca/" interurl="http://www.absound.ca/music/dvd/dvd-main.html">
  <srcname> Sound Online </srcname>
  <form name="DVD / VHS Search">
    <attrgroup>
      <attr name="DVD - Artist/Title" ename="artist|title">
        <domain format="text_box" />
      </attr>
      <attr name="VHS - Artist/Title" ename="artist|title">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
  <form name="List Titles">
    <attrgroup>
      <attr name="List" operator="yes" ename="list">
        <additional>TITLES that start with</additional>
        <operator format="selection_list">
          <value>0-9</value>
          <value>A</value>
          <value>B</value>
          <value>C</value>
          <value>D</value>
          <value>E</value>
          <value>F</value>
          <value>G</value>
          <value>H</value>
          <value>I</value>
          <value>J</value>
          <value>K</value>
          <value>L</value>
          <value>M</value>
          <value>N</value>
          <value>O</value>
          <value>P</value>
          <value>Q</value>
          <value>R</value>
          <value>S</value>
          <value>T</value>
          <value>U</value>
          <value>V</value>
          <value>W</value>
          <value>X</value>
          <value>Y</value>
          <value>Z</value>
        </operator>
        <domain format="selection_list">
          <value>DVD</value>
          <value>VHS</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>