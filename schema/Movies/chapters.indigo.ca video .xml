<source url="http://www.chapters.indigo.ca/" interurl="http://chapters.indigo.ca/search/default.asp?Search_Catalog=Video&amp;Section=video&amp;Cat=&amp;Lang=en&amp;mscssid=QFBM1DM0F83T9N0S9BSHQP5WB08KF9AF&amp;WSID=0405C9BBB141FA8E4B03ABFD96C1E20989F01015">
  <srcname> chapters.indigo.ca video </srcname>
  <form>
    <attrgroup>
      <attr name="Title:" operator="yes" ename="title">
        <operator format="selection_list">
          <value>All</value>
          <value>Any</value>
          <value>Exact</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Actor/Director:" ename="actor|director">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Keyword:" operator="yes" ename="keyword">
        <operator format="selection_list">
          <value>All</value>
          <value>Any</value>
          <value>Exact</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Label:" ename="lebel">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="UPC:" ename="upc">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Sort By:" ename="sort">
        <domain format="selection_list">
          <value>Rank</value>
          <value>Availability</value>
          <value>Title A-Z</value>
          <value>Title Z-A</value>
          <value>Release Date</value>
          <value>Price</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Release Date:" operator="yes" ename="release date">
        <additional>eg.1999</additional>
        <operator format="selection_list">
          <value>Before</value>
          <value>During</value>
          <value>After</value>
        </operator>
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>