<source url="http://www.placeareservation.com/" interurl="http://www.placeareservation.com/">
  <srcname>PlaceAReservation.com</srcname>
  <form>
    <attrgroup>
      <attr name="City" ename="city">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Zip" ename="zip">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Restaurant Name" ename="restaurant name">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Cuisine" ename="cuisine">
        <domain format="selection_list">
          <value>All</value>
          <value>American</value>
          <value>Asian</value>
          <value>Bakery</value>
          <value>Chinese</value>
          <value>Cuban</value>
          <value>Deli</value>
          <value>Eclectic</value>
          <value>French</value>
          <value>Greek</value>
          <value>Italian</value>
          <value>Latin American</value>
          <value>Mediterranean</value>
          <value>Mexican</value>
          <value>Native American</value>
          <value>Seafood</value>
          <value>Steak</value>
          <value>Sushi</value>
          <value>Tex-Mex</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="State" ename="state">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Region" ename="region">
        <domain format="selection_list" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Smoking Preference" ename="smoking preference">
        <domain format="radio_button">
          <value>None</value>
          <value>Non-smoking</value>
          <value>Smoking</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price" ename="price">
        <domain format="radio_button">
          <value>Any</value>
          <value>$5-10</value>
          <value>$11-15</value>
          <value>$16-20</value>
          <value>$21-25</value>
          <value>$21-25</value>
          <value>$26 or more</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Amenities" ename="amenities">
        <domain format="check_box">
          <option>Valet</option>
          <option>Dancing</option>
          <option>Live music</option>
          <option>Parking</option>
          <option>Gift Certificates</option>
          <option>Outdoor seating</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Reservations and Takeout" ename="reservations and takeout">
        <domain format="check_box">
          <option>Reservations</option>
          <option>Takeout</option>
          <option>Delivery</option>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>