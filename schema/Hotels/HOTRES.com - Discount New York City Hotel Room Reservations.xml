<source url="http://www.hotres.com/index.cfm" interurl="http://www.hotres.com/index.cfm">
  <srcname>HOTRES.com - Discount New York City Hotel Room Reservations</srcname>
  <form>
    <attrgroup>
      <attr name="City" ename="city">
        <domain format="selection_list">
          <value>Choose Your City</value>
          <value>New York</value>
          <value>Chicago</value>
          <value>San Diego</value>
          <value>San Francisco</value>
          <value>Los Angeles</value>
          <value>Atlanta</value>
          <value>Washington</value>
          <value>New Orleans</value>
          <value>Miami</value>
          <value>Orlando</value>
          <value>Boston</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Hotel" ename="hotel">
        <domain format="selection_list">
          <value>Choose A City First</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Arrival Date [mm/dd/yyyy]" ename="arrival date">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Nights" ename="nights">
        <domain format="selection_list">
          <value>1</value>
          <value>2</value>
          <value>3</value>
          <value>4</value>
          <value>5</value>
          <value>6</value>
          <value>7</value>
          <value>8</value>
          <value>9</value>
          <value>10</value>
          <value>11</value>
          <value>12</value>
          <value>13</value>
          <value>14</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Rooms" ename="rooms">
        <domain format="selection_list">
          <value>1</value>
          <value>2</value>
          <value>3</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Adults" ename="adults">
        <domain format="selection_list">
          <value>1</value>
          <value>2</value>
          <value>3</value>
          <value>4</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Children" ename="children">
        <domain format="selection_list">
          <value>0</value>
          <value>1</value>
          <value>2</value>
          <value>3</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>