<source url="http://krieger.smartwebconcepts.com/" interurl="http://krieger.smartwebconcepts.com/frames.cfm">
  <srcname>Krieger</srcname>
  <form>
    <attrgroup>
      <attr name="Year" ename="year">
        <domain format="selection_list">
          <value>Select a Year</value>
          <value>2003</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Make" ename="make">
        <domain format="selection_list">
          <value>Select a Manufacturer</value>
          <value>Chevrolet</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model" ename="model">
        <domain format="selection_list">
          <value>Select an Available Model</value>
          <value>Cavalier</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Body" ename="body">
        <domain format="selection_list">
          <value>Select a Body Style</value>
          <value>Coupe</value>
          <value>Sedan</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price" ename="price">
        <domain format="selection_list">
          <value>Select a Range</value>
          <value>less than $15,000</value>
          <value>$15,001 - $20,000</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Zip Code" ename="zip code">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>