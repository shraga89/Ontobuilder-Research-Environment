<source url="http://www.hulingbrosautoloanstore.com/" interurl="http://www.hulingbrosautoloanstore.com/en_US/CombinedVehicleSearch_1.chtml">
  <srcname>Huling Bros. Auto Center</srcname>
  <form>
    <attrgroup>
      <attr name="SEARCH:" ename="search">
        <domain format="selection_list">
          <value>New</value>
          <value>Pre-Owned</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="MAKE:" ename="make">
        <domain format="selection_list">
          <value>Buick</value>
          <value>Chevrolet</value>
          <value>Chrysler</value>
          <value>GMC</value>
          <value>Jeep</value>
          <value>Pontiac</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="MODEL/TYPE:" ename="model|type">
        <domain format="selection_list">
          <value>Any</value>
          <value>CAR</value>
          <value>SUV</value>
          <value>TRUCK</value>
          <value>VAN</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>