          <value>Porsche</value>
          <value>Rolls-Royce</value>
          <value>Saab</value>
          <value>Saturn</value>
          <value>Subaru</value>
          <value>Suzuki</value>
          <value>Toyota</value>
          <value>Volkswagen</value>
          <value>Volvo</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model:" ename="model">
        <domain format="selection_list">
          <value>Any</value>
          <value>Accord</value>
          <value>Civic</value>
          <value>CRV</value>
          <value>Element</value>
          <value>Insight</value>
          <value>Odyssey</value>
          <value>Passport</value>
          <value>Pilot</value>
          <value>Prelude</value>
          <value>S2000</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Style:" ename="style">
        <domain format="selection_list">
          <value>Any style</value>
          <value>2dr Cpe DX</value>
          <value>2dr Cpe EX</value>
          <value>2dr Cpe HX</value>
          <value>2dr Cpe LX</value>
          <value>4 Dr VP Sedan</value>
          <value>4dr Sdn DX</value>
          <value>4dr Sdn EX</value>
          <value>4dr Sdn LX</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Minimum monthly mileage remaining:" ename="">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Minimum total mileage remaining:" ename="">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Lease period:" level="1" ename="lease period">
        <input>
          <operator name="" ename="" />
          <domain format="text_box" />
        </input>
        <input>
          <operator name="to" ename="to" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Monthly payment:" level="1" ename="monthly payment">
        <input>
          <operator name="" ename="" />
          <domain format="text_box" />
        </input>
        <input>
          <operator name="to" ename="to" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" level="1" ename="">
        <input>
          <operator name="Zip code" ename="zip code" />
          <domain format="text_box" />
        </input>
        <input>
          <operator name="miles" ename="miles" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Region:" ename="region">
        <domain format="selection_list">
          <value>Select a region</value>
          <value>Northeast</value>
          <value>Northwest</value>
          <value>Midwest</value>
          <value>Southwest</value>
          <value>South</value>
          <value>Pacific</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <domain format="check_box">
          <option>Recent postings only</option>
          <option>Show cars with incentive only</option>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>