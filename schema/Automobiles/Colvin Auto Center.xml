<source url="http://www.colvinauto.com/" interurl="http://www.colvinauto.com/SearchUsedFram.htm">
  <srcname>Colvin Auto Center</srcname>
  <form>
    <attrgroup>
      <attr name="Make" ename="make">
        <domain format="selection_list">
          <value>All</value>
          <value>CHEVROLET</value>
          <value>DODGE</value>
          <value>FORD</value>
          <value>HONDA</value>
          <value>HYUNDAI</value>
          <value>INFINITI</value>
          <value>JEEP</value>
          <value>LINCOLN</value>
          <value>MAZDA</value>
          <value>MERCURY</value>
          <value>NISSAN</value>
          <value>PONTIAC</value>
          <value>SUBARU</value>
          <value>TOYOTA</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price Range" ename="price range">
        <domain format="selection_list">
          <value>Any</value>
          <value>1 - 5,000</value>
          <value>5,000 - 10,000</value>
          <value>10,000 - 15,000</value>
          <value>15,000 - 20,000</value>
          <value>20,000 - 25,000</value>
          <value>25,000 +</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Year is" level="1" ename="year">
        <input>
          <operator name="From" ename="from" />
          <domain format="selection_list">
            <value>All Years</value>
            <value>Newer Than</value>
            <value>Older Than</value>
            <value>Exactly</value>
          </domain>
        </input>
        <input>
          <operator name="To" ename="to" />
          <domain format="selection_list">
            <value>Any</value>
            <value>2003</value>
            <value>2002</value>
            <value>2001</value>
            <value>2000</value>
            <value>1999</value>
            <value>1998</value>
            <value>1997</value>
            <value>1996</value>
            <value>1995</value>
            <value>1994</value>
            <value>1993</value>
            <value>1992</value>
            <value>1991</value>
            <value>1990</value>
            <value>1989</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model" ename="model">
        <domain format="selection_list">
          <value>Choose a Model [optional]</value>
          <value>ACCORD</value>
          <value>CIVIC</value>
          <value>CR-V</value>
          <value>PASSPORT</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Or, enter some keywords:" ename="keywords">
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Body Type" ename="body type">
        <domain format="selection_list">
          <value>Choose a Body Type</value>
          <value>2 Door Cars</value>
          <value>4 Door Cars</value>
          <value>Convertibles</value>
          <value>Minivan/Vans</value>
          <value>Pickup/Trucks</value>
          <value>Sports Utility Vehicles</value>
          <value>Wagons</value>
          <value>Other</value>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>