<source url="http://classifieds.carprices.com/" interurl="http://inventory.carprices.com/advanced.html">
  <srcname>CarPrices.com</srcname>
  <form>
    <attrgroup>
      <attr name="Make" ename="make">
        <additional>Choose a Make and Model</additional>
        <domain format="selection_list">
          <value>Select A Make</value>
          <value>Acura</value>
          <value>Audi</value>
          <value>BMW</value>
          <value>Buick</value>
          <value>Cadillac</value>
          <value>Chevrolet</value>
          <value>Chrysler</value>
          <value>Daewoo</value>
          <value>Dodge</value>
          <value>Ford</value>
          <value>GMC</value>
          <value>Honda</value>
          <value>Hyundai</value>
          <value>Infiniti</value>
          <value>Isuzu</value>
          <value>Jaguar</value>
          <value>Jeep</value>
          <value>Kia</value>
          <value>Land Rover</value>
          <value>Lexus</value>
          <value>Lincoln</value>
          <value>Mazda</value>
          <value>Mercedes-Benz</value>
          <value>Mercury</value>
          <value>Mitsubishi</value>
          <value>Nissan</value>
          <value>Oldsmobile</value>
          <value>Plymouth</value>
          <value>Pontiac</value>
          <value>Saab</value>
          <value>Saturn</value>
          <value>Subaru</value>
          <value>Suzuki</value>
          <value>Toyota</value>
          <value>Volkswagen</value>
          <value>Volvo</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model" ename="model">
        <additional>Choose a Make and Model</additional>
        <domain format="selection_list" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="" ename="">
        <additional>Choose a Make and Model</additional>
        <domain format="check_box">
          <option>New</option>
          <option>Used</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Body Style(s):" ename="body styles">
        <additional>Narrow Your Search</additional>
        <domain format="check_box">
          <option>Convertible</option>
          <option>Coupe</option>
          <option>Hatchback</option>
          <option>Pickup</option>
          <option>Suv</option>
          <option>Sedan</option>
          <option>Van</option>
          <option>Wagon</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price Range" level="1" ename="price range">
        <additional>Narrow Your Search</additional>
        <input>
          <operator name="From" ename="from" />
          <domain format="selection_list" />
        </input>
        <input>
          <operator name="To" ename="to" />
          <domain format="selection_list" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Year Range" level="1" ename="year range">
        <additional>Narrow Your Search</additional>
        <input>
          <operator name="From" ename="from" />
          <domain format="selection_list" />
        </input>
        <input>
          <operator name="To" ename="to" />
          <domain format="selection_list" />
        </input>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Car Mileage:" ename="car mileage">
        <additional>Narrow Your Search</additional>
        <domain format="selection_list">
          <value>less than 10,000 miles</value>
          <value>less than 20,000 miles</value>
          <value>less than 30,000 miles</value>
          <value>less than 40,000 miles</value>
          <value>less than 50,000 miles</value>
          <value>less than 60,000 miles</value>
          <value>less than 80,000 miles</value>
          <value>less than 100,000 miles</value>
          <value>less than 120,000 miles</value>
          <value>Doesn't Matter</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Exterior Color?" ename="exterior color">
        <additional>Narrow Your Search</additional>
        <domain format="check_box">
          <option>Black</option>
          <option>White</option>
          <option>Blue</option>
          <option>Green</option>
          <option>Silver/Gray</option>
          <option>Gold/Brown</option>
          <option>Yellow</option>
          <option>Orange</option>
          <option>Purple</option>
          <option>Red</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Tell Us Where to Search" level="1" ename="Tell Us Where to Search">
        <input>
          <operator name="Distance" ename="distance" />
          <domain format="selection_list" />
        </input>
        <input>
          <operator name="from Zip Code" ename="zip code" />
          <domain format="text_box" />
        </input>
      </attr>
    </attrgroup>
  </form>
</source>