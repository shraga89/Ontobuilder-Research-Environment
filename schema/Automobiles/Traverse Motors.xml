<source url="http://www.traversemotors.com" interurl="http://www.traversemotors.com/index.cfm?action=inventorysearch&amp;subaction=searchdisplay&amp;inventorytype=used">
  <srcname>Traverse Motors</srcname>
  <form>
    <attrgroup>
      <attr name="Search for:" ename="search for">
        <domain format="radio_button">
          <value>New</value>
          <value>Preowned</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Search by:" ename="search by">
        <domain format="radio_button">
          <value>Make / Model</value>
          <value>Body Style</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Make:" ename="make">
        <domain format="selection_list">
          <value>Select Make</value>
          <value>Audi</value>
          <value>Buick</value>
          <value>Chevrolet</value>
          <value>Chrysler</value>
          <value>Dodge</value>
          <value>Ford</value>
          <value>GMC</value>
          <value>Honda</value>
          <value>Isuzu</value>
          <value>Jeep</value>
          <value>Lexus</value>
          <value>Mazda</value>
          <value>Mercury</value>
          <value>Mitsubishi</value>
          <value>Nissan</value>
          <value>Oldsmobile</value>
          <value>Plymouth</value>
          <value>Pontiac</value>
          <value>Saturn</value>
          <value>Subaru</value>
          <value>Toyota</value>
          <value>Volkswagen</value>
          <value>Volvo</value>
          <value>All Makes</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model:" ename="model">
        <domain format="selection_list">
          <value>A4</value>
          <value>Allroad</value>
          <value>S6</value>
          <value>All Models</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Price Range:" level="1" ename="price range">
        <input>
          <operator name="From:" ename="from" />
          <domain format="selection_list">
            <value>Any</value>
            <value>$1,000</value>
            <value>$5,000</value>
            <value>$10,000</value>
            <value>$20,000</value>
            <value>$30,000</value>
            <value>$40,000</value>
          </domain>
        </input>
        <input>
          <operator name="To:" ename="to" />
          <domain format="selection_list">
            <value>Any</value>
            <value>$1,000</value>
            <value>$5,000</value>
            <value>$10,000</value>
            <value>$20,000</value>
            <value>$30,000</value>
            <value>$40,000</value>
          </domain>
        </input>
      </attr>
    </attrgroup>
  </form>
</source>