<source url="http://cartalk.cars.com/" interurl="http://cartalk.cars.com/Tools/atm-search.pl">
  <srcname>Ask the Manufacturer Search</srcname>
  <form>
    <attrgroup>
      <attr name="Manufacturer:" ename="manufacturer">
        <domain format="selection_list">
          <value>(All makes)</value>
          <value>Acura</value>
          <value>Chrysler</value>
          <value>Dodge</value>
          <value>Hemmings</value>
          <value>Honda</value>
          <value>Hyundai</value>
          <value>Infiniti</value>
          <value>Isuzu</value>
          <value>Jeep</value>
          <value>Land Rover</value>
          <value>Lexus</value>
          <value>Mazda</value>
          <value>Mercedes</value>
          <value>Mitsubishi</value>
          <value>Nissan</value>
          <value>Plymouth</value>
          <value>Saab</value>
          <value>Subaru</value>
          <value>Toyota</value>
          <value>Volvo</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Date:" ename="date">
        <domain format="selection_list">
          <value>(All dates)</value>
          <value>Past Week</value>
          <value>Past Month</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Keywords:" ename="keywords">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>