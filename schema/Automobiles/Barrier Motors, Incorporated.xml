<source url="http://www.barriermotors.com/" interurl="http://www.barriermotors.com/f_quickquote.cgi">
  <srcname>Barrier Motors, Incorporated</srcname>
  <form>
    <attrgroup>
      <attr name="Manufacturer:" ename="manufacturer">
        <additional>Choose a Vehicle</additional>
        <domain format="selection_list">
          <value>Select One</value>
          <value>Audi</value>
          <value>Mercedes-Benz</value>
          <value>Porsche</value>
          <value>Saab</value>
          <value>Volvo</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Exterior Color:" ename="exterior color">
        <additional>Choose a Vehicle</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Interior Color:" ename="interior color">
        <additional>Choose a Vehicle</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Additional Options:" ename="additional options">
        <additional>Choose a Vehicle</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Purchase Time:" ename="purchase time">
        <additional>Purchase Information</additional>
        <domain format="selection_list">
          <value>Cash</value>
          <value>Finance</value>
          <value>Lease</value>
          <value>I'm undecided</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Payment Method:" ename="payment method">
        <additional>Purchase Information</additional>
        <domain format="selection_list">
          <value>I plan to purchase within the next 72 hours.</value>
          <value>I plan to purchase within the next 2 weeks.</value>
          <value>I plan to purchase within the next month.</value>
          <value>I'm undecided</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Manufacturer:" ename="manufacturer">
        <additional>Trade-In Information</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Year:" ename="year">
        <additional>Trade-In Information</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model:" ename="model">
        <additional>Trade-In Information</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Miles:" ename="miles">
        <additional>Trade-In Information</additional>
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
</source>