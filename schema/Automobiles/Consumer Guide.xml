          <value>Sports And Gt Car</value>
          <value>Sports Car</value>
          <value>Sporty Coupe</value>
          <value>Subcompact Car</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Model Name" ename="model name">
        <domain format="text_box" />
      </attr>
    </attrgroup>
  </form>
