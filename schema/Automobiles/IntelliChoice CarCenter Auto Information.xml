<source url="http://www.intellichoice.com/" interurl="http://www.intellichoice.com/search/criteriaSearch-fs.cfm">
  <srcname>IntelliChoice CarCenter Auto Information</srcname>
  <form>
    <attrgroup>
      <attr name="Year." ename="year">
        <domain format="check_box">
          <option>2004</option>
          <option>2003</option>
          <option>2002</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="What kind of car would you like?" ename="">
        <domain format="check_box">
          <option>Sedans</option>
          <option>Convertibles</option>
          <option>Coupes</option>
          <option>Station Wagons</option>
          <option>SUVs</option>
          <option>Hatchbacks</option>
          <option>Trucks</option>
          <option>Vans</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Vehicle Price" ename="price">
        <domain format="selection_list">
          <value>Select a Price</value>
          <value>Doesn't Matter</value>
          <value>$10,000 - $15,000</value>
          <value>$15,000 - $20,000</value>
          <value>$20,000 - $25,000</value>
          <value>$25,000 - $30,000</value>
          <value>$30,000 - $35,000</value>
          <value>$35,000 - $40,000</value>
          <value>$40,000 - $45,000</value>
          <value>$45,000 - $50,000</value>
          <value>$50,000 and more</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Overall Value Rating" ename="overall value rating">
        <domain format="selection_list">
          <value>Select a Value</value>
          <value>Doesn't Matter</value>
          <value>Excellent</value>
          <value>Better Than Average</value>
          <value>Average</value>
          <value>Poor</value>
          <value>Worse Than Average</value>
          <value>Insufficient Info</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Awards" ename="awards">
        <domain format="selection_list">
          <value>Select an Award</value>
          <value>Doesn't Matter</value>
          <value>BOVY</value>
          <value>Wards</value>
          <value>Polk</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Passengers" ename="passengers">
        <additional>Vehicle Size</additional>
        <domain format="selection_list">
          <value>-</value>
          <value>2</value>
          <value>3-5</value>
          <value>6+</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Doors" ename="doors">
        <additional>Vehicle Size</additional>
        <domain format="selection_list">
          <value>-</value>
          <value>2</value>
          <value>3</value>
          <value>4</value>
          <value>5</value>
          <value>6</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Head Room" ename="head room">
        <additional>Vehicle Size</additional>
        <domain format="radio_button">
          <value>Important</value>
          <value>Not Important</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Leg Room" ename="leg room">
        <additional>Vehicle Size</additional>
        <domain format="radio_button">
          <value>Important</value>
          <value>Not Important</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Cargo Capacity" ename="cargo capacity">
        <additional>Vehicle Size</additional>
        <domain format="radio_button">
          <value>Important</value>
          <value>Not Important</value>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Transmission" ename="transmisson">
        <domain format="check_box">
          <option>Automatic</option>
          <option>Manual</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Safety Features" ename="safety features">
        <domain format="check_box">
          <option>Driver Side Airbag</option>
          <option>Passenger Side Airbag</option>
          <option>Anti-Lock Brakes</option>
          <option>Passenger Air Bag Cutoff</option>
          <option>1997 Side Impact</option>
          <option>Daytime Running Lights</option>
        </domain>
      </attr>
    </attrgroup>
    <attrgroup>
      <attr name="Optional Features" ename="optional features">
        <domain format="check_box">
          <option>Cruise Control</option>
          <option>Cup Holder</option>
          <option>Power Door Locks</option>
          <option>Sunroof</option>
          <option>Leather Seats</option>
          <option>Power Windows</option>
        </domain>
      </attr>
    </attrgroup>
  </form>
</source>