<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE algorithms SYSTEM "dtds/algorithms.dtd">
<algorithms>
	<preprocessing>
		<parameters>
			<parameter>
				<name>stopTerms</name>
				<value>of,a,to,the,and,or,this,those,these,which,what,where,do,does,done,did,among,in,for</value>
			</parameter>
			<parameter>
				<name>stopTermSeparator</name>
				<value>,</value>
				<default>,</default>
			</parameter>
            <parameter>
				<name>mergeWords</name>
				<value>true</value>
				<default>true</default>
			</parameter>
			<parameter>
				<name>caseSensitive</name>
				<value>false</value>
				<default>false</default>
			</parameter>
		</parameters>
    </preprocessing>
	<algorithm name="Term Match">
		<class>com.modica.ontology.algorithm.TermAlgorithm</class>
		<parameters>
			<parameter>
				<name>symmetric</name>
				<value>true</value>
				<default>true</default>
			</parameter>
			<parameter>
				<name>useThesaurus</name>
				<value>true</value>
				<default>true</default>
			</parameter>			
			<parameter>
				<name>useSoundex</name>
				<value>true</value>
				<default>true</default>
			</parameter>			
			<parameter>
				<name>wordLabelWeight</name>
				<value>0.40</value>
				<default>0.25</default>
			</parameter>			
			<parameter>
				<name>stringLabelWeight</name>
				<value>0.10</value>
				<default>0.25</default>
			</parameter>			
			<parameter>
				<name>wordNameWeight</name>
				<value>0.40</value>
				<default>0.25</default>
			</parameter>			
			<parameter>
				<name>stringNameWeight</name>
				<value>0.10</value>
				<default>0.25</default>
			</parameter>
			<parameter>
				<name>maxCommonSubStringWeight</name>
				<value>0.50</value>
				<default>0.50</default>
			</parameter>	
			<parameter>
				<name>nGramWeight</name>
				<value>0.50</value>
				<default>0.50</default>
			</parameter>
			<parameter>
				<name>nGram</name>
				<value>2</value>
				<default>2</default>
			</parameter>				
		</parameters>
	</algorithm>
	<algorithm name="Value Match">
		<class>com.modica.ontology.algorithm.ValueAlgorithm</class>
		<parameters>
			<parameter>
				<name>symmetric</name>
				<value>true</value>
				<default>true</default>
			</parameter>
			<parameter>
				<name>useThesaurus</name>
				<value>true</value>
				<default>true</default>
			</parameter>			
			<parameter>
				<name>useSoundex</name>
				<value>true</value>
				<default>true</default>
			</parameter>
<parameter>
				<name>wordLabelWeight</name>
				<value>0.40</value>
				<default>0.25</default>
			</parameter>			
			<parameter>
				<name>stringLabelWeight</name>
				<value>0.10</value>
				<default>0.25</default>
			</parameter>			
			<parameter>
				<name>wordNameWeight</name>
				<value>0.40</value>
				<default>0.25</default>
			</parameter>			
			<parameter>
				<name>stringNameWeight</name>
				<value>0.10</value>
				<default>0.25</default>
			</parameter>
			<parameter>
				<name>maxCommonSubStringWeight</name>
				<value>0.50</value>
				<default>0.50</default>
			</parameter>	
			<parameter>
				<name>nGramWeight</name>
				<value>0.50</value>
				<default>0.50</default>
			</parameter>
			<parameter>
				<name>nGram</name>
				<value>2</value>
				<default>2</default>
			</parameter>			
		</parameters>
	</algorithm>    
	<algorithm name="Term and Value Match">
		<class>com.modica.ontology.algorithm.TermValueAlgorithm</class>
        <parameters>
			<parameter>
				<name>termWeight</name>
				<value>0.5</value>
				<default>0.5</default>
			</parameter>
			<parameter>
				<name>valueWeight</name>
				<value>0.5</value>
				<default>0.5</default>
			</parameter>        
        </parameters>
	</algorithm>        
	<algorithm name="Graph Match">
		<class>com.modica.ontology.algorithm.GraphAlgorithm</class>
		<parameters>
			<parameter>
				<name>termWeight</name>
				<value>0.33</value>
				<default>0.33</default>
			</parameter>
			<parameter>
				<name>valueWeight</name>
				<value>0.33</value>
				<default>0.33</default>
			</parameter>        
			<parameter>
				<name>graphWeight</name>
				<value>0.34</value>
				<default>0.34</default>
			</parameter>             
			<parameter>
				<name>parentsWeight</name>
				<value>0.5</value>
				<default>0.5</default>
			</parameter>
			<parameter>
				<name>siblingsWeight</name>
				<value>0.5</value>
				<default>0.5</default>
			</parameter>      
        </parameters>
	</algorithm>            
	<algorithm name="Precedence Match">
		<class>com.modica.ontology.algorithm.PrecedenceAlgorithm</class>
		<parameters>
			<parameter>
				<name>termWeight</name>
				<value>0.33</value>
				<default>0.33</default>
			</parameter>
			<parameter>
				<name>valueWeight</name>
				<value>0.33</value>
				<default>0.33</default>
			</parameter>
			<parameter>
				<name>precedenceWeight</name>
				<value>0.34</value>
				<default>0.34</default>
			</parameter>                     
			<parameter>
				<name>precedeWeight</name>
				<value>0.5</value>
				<default>0.5</default>
			</parameter>
			<parameter>
				<name>succeedWeight</name>
				<value>0.5</value>
				<default>0.5</default>
			</parameter>  
        </parameters>
	</algorithm>
	<algorithm name="Combined Match">
		<class>com.modica.ontology.algorithm.CombinedAlgorithm</class>
		<parameters>
			<parameter>
				<name>termWeight</name>
				<value>0.25</value>
				<default>0.25</default>
			</parameter>
			<parameter>
				<name>valueWeight</name>
				<value>0.25</value>
				<default>0.25</default>
			</parameter>  			
			<parameter>
				<name>graphWeight</name>
				<value>0.15</value>
				<default>0.25</default>
			</parameter>  			
			<parameter>
				<name>precedenceWeight</name>
				<value>0.35</value>
				<default>0.25</default>
			</parameter>  			
        </parameters>
	</algorithm>
<algorithm name="SimilarityFlooding Match">
		<class>com.modica.ontology.algorithm.SimilarityFloodingAlgorithm</class>
		<parameters>
			<parameter>
				<name>epsilon</name>
				<value>0.05</value>
				<default>0.05</default>
			</parameter>
			<parameter>
				<name>Fixpoint Type</name>
				<value>1</value>
				<default>1</default>
			</parameter>
        </parameters>
	</algorithm>
</algorithms>